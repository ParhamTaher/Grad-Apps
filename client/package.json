{
    "name": "client",
    "version": "0.1.0",
    "private": true,
    "proxy": {
        "/tickets": {
            "target": "http://localhost:5000"
        },
        "/users/signup": {
            "target": "http://localhost:3002"
        },
        "/users/login": {
            "target": "http://localhost:3002"
        },
        "/applicants": {
            "target": "http://localhost:3002"
        }
    },
    "dependencies": {
        "axios": "^0.18.0",
        "concurrently": "^3.5.1",
        "localforage": "^1.7.1",
        "react": "^16.2.0",
        "react-bootstrap": "^0.32.1",
        "react-cookie": "^2.1.4",
        "react-dom": "^16.2.0",
        "react-redux": "^5.0.7",
        "react-router-bootstrap": "^0.24.4",
        "react-router-dom": "^4.2.2",
        "react-router-redux": "^5.0.0-alpha.9",
        "react-scripts": "1.1.1",
        "redux": "^3.7.2",
        "redux-form": "^7.3.0",
        "redux-persist": "^5.9.1",
        "redux-thunk": "^2.2.0"
    },
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test --env=jsdom",
        "eject": "react-scripts eject",
        "client": "npm run start",
        "serverTicket": "npm start --prefix ../server/ticket",
        "serverUser": "npm start --prefix ../server/users",
        "serverGapf": "npm start --prefix ../server/gapf",
        "dev": "concurrently \"npm run serverTicket\" \"npm run serverUser\" \"npm run serverGapf\" \"npm run client\""
    }
}
